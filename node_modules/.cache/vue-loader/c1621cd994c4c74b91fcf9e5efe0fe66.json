C;AACvB;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACZ,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;AACnB","file":"/home/sp/Documents/Vue3Firebase/serhii-blog2FB/src/views/Details.vue","sourceRoot":"","sourcesContent":["<template>\n  <div v-if=\"error\">{{ error }}</div>\n  <div v-if=\"post\" class=\"post\">\n    <h3>{{ post.title }}</h3>\n    <p class=\"pre\">{{ post.body }}</p>\n    <button @click=\"handleClick\" class=\"delete\">delete post</button>\n  </div>\n  <div v-else><Spinner /></div>\n</template>\n\n<script>\nimport getPost from '../composables/getPost';\nimport Spinner from '../components/Spinner.vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport { projectFirestore } from '../firebase/config';\n\nexport default {\n  props: ['id'],\n  components: { Spinner },\n  setup(props) {\n    const route = useRoute();\n    const router = useRouter();\n    console.log(route);\n\n    const { post, error, load } = getPost(route.params.id);\n    load();\n\n    const handleClick = async () => {\n      await projectFirestore.collection('posts').doc(props.id).delete();\n\n      router.push({ name: 'Home' });\n    };\n\n    return { post, error, handleClick };\n  },\n};\n</script>\n\n<style>\n.post {\n  max-width: 1200px;\n  margin: 0 auto;\n}\n.post p {\n  color: #444;\n  line-height: 1.5rem;\n  margin-top: 40px;\n}\n.pre {\n  white-space: pre-wrap;\n}\nbutton.delete {\n  margin: 10px auto;\n}\n</style>\n"]}]}